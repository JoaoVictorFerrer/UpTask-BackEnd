{
    "compilerOptions": {
      "baseUrl": "./src",
      "outDir": "./dist",
      "rootDir": "./src",
      "lib": ["esnext"],
      "target": "ESNext",
      "moduleResolution": "NodeNext",
      "module": "NodeNext",
      "strict": false,
      "sourceMap": true,
      "esModuleInterop": true,
      "declaration": true,
      "paths": {
        "@models/*": ["models/*"]
      }
    },
    "include": ["src/**/*"],
  }